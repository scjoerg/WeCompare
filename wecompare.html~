     $(document).ready(function(){
     $("#urlinput").keypress(function(event) {
       if ( event.which == 13 ) {
         event.preventDefault();
         if ('urlinput'.substring(0,7) != "http://"){
         	'urlinput'="http://"+'urlinput';
         }
         document.getElementById('oFrame').src = document.getElementById('urlinput').value;
       }
     
	});
     });
   </script>
